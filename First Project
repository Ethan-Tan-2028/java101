public class Main {

  public static void main(String[] args)
    {
   int[] arr = {4, 3, 2, 8, 3};
int numbertoCount = 3;
      int count=0;
       for (int i = 0; i < arr.length; i++){
        if (numbertoCount==arr[i])
        {
       count++;
        }
       }
    System.out.println(count);
}
}
